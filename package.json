{
  "name": "poe-dat-export",
  "version": "1.3.1",
  "description": "Library for exporting .dat files as tables from the content.ggpk of the game Path Of Exile",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "keywords": [
    "Typescript",
    "Path of Exile",
    "poe",
    "content.ggpk",
    "dat"
  ],
  "license": "MIT",
  "repository": "git://github.com/moepmoep12/poe-dat-export.git",
  "author": {
    "name": "Andreas Schmidt",
    "email": "moepmoep12@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "dotenv -e .env mocha",
    "test:ci": "mocha",
    "lint": "eslint src/ tests/",
    "prettier": "prettier --write \"**/*.ts\"",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "debug": "^4.3.4",
    "expiry-map": "^2.0.0",
    "fnv-plus": "^1.3.1",
    "ooz-wasm": "^1.0.0",
    "pathofexile-dat-schema": "^3.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.7",
    "@types/fnv-plus": "^1.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "Release <%= nextRelease.version %> [skip ci]",
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  }
}
